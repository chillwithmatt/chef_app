<h1>Dishes#new</h1>
<p>Find me in app/views/dishes/new.html.erb</p>

<%= form_for(@dish) do |f| %>
	<%#= render 'shared/error_messages' %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit "Submit a Dish" %>
<% end %>